
pipeline {
    agent any
    environment {
       PATH = "/opt/maven3/bin:$PATH"
       }
    stages {
        stage('git checkout') {
            steps { 
                git credentialid: 'jenkins-java'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
